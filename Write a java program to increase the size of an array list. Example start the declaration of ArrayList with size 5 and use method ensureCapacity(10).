import java.util.*;
public class Code13 {
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<>(5);
        list.add("String1");
        list.add("String2");
        list.add("String3");
        list.add("String4");
        list.add("String5");
        System.out.println("ArrayList before increasing capacity:");
        for (String str : list) {
            System.out.println(str);
        }
        list.ensureCapacity(10);
        list.add("String6");
        list.add("String7");
        System.out.println("ArrayList after increasing capacity:");
        for (String str : list) {
            System.out.println(str);
        }
    }
}
